<body>
    <form action="http://localhost:3000/saveUser" method="POST">
      <div class="form" >
        <div class="title">BIENVENUE</div>
        <div class="subtitle">Créons votre compte !</div>

        <div class="input-container ic1">
          <input placeholder="" type="text" class="input" id="firstname" name="nom">
          <div class="cut"></div>
          <label class="iLabel" for="firstname">Nom et Prénoms</label>
        </div>

        <div class="input-container ic2">
          <input placeholder="" type="text" class="input" id="email" name="email">
          <div class="cut cut-short"></div>
          <label class="iLabel" for="email">Adresse Email</label>
        </div>

        <div class="input-container ic2">
            <input placeholder="" type="text" class="input" id="tel" name="telephone">
            <div class="cut cut-short"></div>
            <label class="iLabel" for="telephone">Téléphone</label>
          </div>

          <div class="input-container ic2">
            <input placeholder="" type="password" class="input" id="tel" name="password">
            <div class="cut cut-short"></div>
            <label class="iLabel" for="password">Mot de Passe</label>
          </div>
        <button class="submit" type="submit">submit</button>
        
        
      </div>
    </form>

</body>


